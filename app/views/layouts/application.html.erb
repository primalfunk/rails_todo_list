<!DOCTYPE html>
<html>
  <head>
    <title>Todolist</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', "https://fonts.googleapis.com/css?family=Amatic+SC|IM+Fell+French+Canon+SC|Titillium+Web|Indie+Flower:300" %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="bodywrap">
      <%= render 'layouts/navbar' %>
    </div>
    <%= yield %>
  </body>
</html>
